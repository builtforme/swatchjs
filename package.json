  "dependencies": {
    "function-arguments": "^1.0.8",
    "joi": "^13.6.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "coveralls": "^3.0.2",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mocha-eslint": "^4.1.0",
    "nyc": "^13.0.1"
